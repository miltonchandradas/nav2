 <mvc:View controllerName="com.sap.nav2.controller.App"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
  <App id="idAppControl" >
    <pages>
      <Page title="{i18n>title}">
        <content>

          <Button text="Navigate to App 1" press=".onNav">
